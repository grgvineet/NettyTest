<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="org.kde.nettytest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.kde.nettytest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.kde.nettytest.Server"
            android:label="@string/title_activity_server" >
        </activity>
        <activity
            android:name="org.kde.nettytest.Client"
            android:label="@string/title_activity_client" >
        </activity>
    </application>

</manifest>
